{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run iotfreestyle-freestyle",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/iotfreestyle-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/gve/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/freestyle",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/iotfreestyle/freestyle\"}"
      },
      "envFile": "${workspaceFolder}/freestyle/.env1",
      "preLaunchTask": "Build iotfreestyle"
    }
  ]
}